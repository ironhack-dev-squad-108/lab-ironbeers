<div class="index-container">
  
  <ul class="product">
  {{#each beers}}
  <li class="product-box">
    <img src="{{this.image_url}}" alt="{{this}}" height = "150px">
    <h1>{{this.name}}</h1>
    <h4>{{this.tagline}}</h4>    
    <p>{{this.description}}</p>
    </li>
  {{/each}}
  </ul>

</div>

/images/{{this}}.jpg
