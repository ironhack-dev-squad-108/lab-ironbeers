<div>
  {{#each beers}}
<div id="beerInfo">
    <img src="{{this.image_url}}">
    <div>
        <h2>{{this.name}}</h2>
        <h3>{{this.tagline}}</h3>
        <p>{{this.description}}</p>

        <h4>Food Pairing</h4>
        <p><ul>{{#each this.food_pairing}}<li>{{this}}</li>{{/each}}</ul></p>
        <p><b>Brewers Tips: </b>{{brewers_tips}}</p>
    </div>
      {{/each}}
</div>